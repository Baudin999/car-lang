# Web Example

type Response a =
    Body: a

type Address =
    Street: String

@api: something
type Customer =
    Name: String
    Number: String
    CustomerNumber: String
    DateOfBirth: Date
    CustomerAddress: Address

alias CustomerResponse = Response Customer

type Error =
    Code: String
    Message: String

type NotFound extends Error

type OtherError extends Error


data ApiResponse =
    | CustomerResponse
    | NotFound
    | OtherError
