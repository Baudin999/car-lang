  <xsd:element name="Docent_DocentenNummer" nillable="false">
    <xsd:annotation></xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:minLength value="1" />
        <xsd:maxLength value="100" />
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="Docent_BevoegdVoor" nillable="false">
    <xsd:annotation></xsd:annotation>
    <xsd:complexType>
