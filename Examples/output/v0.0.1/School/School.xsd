    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:minLength value="1" />
        <xsd:maxLength value="100" />
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:complexType name="Adres">
    <xsd:all>
      <xsd:element ref="self:Adres_Straat" minOccurs="1" />
      <xsd:element ref="self:Adres_Huisnummer" minOccurs="1" />
