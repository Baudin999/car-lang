      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="Klas">
    <xsd:all>
      <xsd:element ref="self:Klas_Studenten" minOccurs="1" />
      <xsd:element ref="self:Klas_Docenten" minOccurs="1" />
    </xsd:all>
  </xsd:complexType>
