  </xsd:element>
  <xsd:element name="Student_ActiefAdres" type="self:Adres" nillable="false">
    <xsd:annotation></xsd:annotation>
  </xsd:element>
  <xsd:complexType name="Student">
    <xsd:all>
      <xsd:element ref="self:Student_Nummer" minOccurs="1" />
      <xsd:element ref="self:Student_HeeftVakken" minOccurs="1" />
      <xsd:element ref="self:Student_Voornaam" minOccurs="1" />
      <xsd:element ref="self:Student_Achternaam" minOccurs="1" />
