      <xsd:documentation>A skill is something a character can do</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element ref="self:Skill_Name" minOccurs="1" />
      <xsd:element ref="self:Skill_Description" minOccurs="1" />
