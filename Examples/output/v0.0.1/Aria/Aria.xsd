    <xsd:all>
      <xsd:element ref="self:Statistic_Stat" minOccurs="1" />
      <xsd:element ref="self:Statistic_Description" minOccurs="1" />
      <xsd:element ref="self:Statistic_XP" minOccurs="1" />
      <xsd:element ref="self:Statistic_Value" minOccurs="1" />
    </xsd:all>
