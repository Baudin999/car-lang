    <xsd:annotation></xsd:annotation>
  </xsd:element>
  <xsd:element name="Buyable_Bought" nillable="false">
    <xsd:annotation></xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:boolean"></xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="Buyable_Skilled" nillable="false">
    <xsd:annotation></xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:boolean"></xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="Buyable_Professional" nillable="false">
    <xsd:annotation></xsd:annotation>
    <xsd:simpleType>
