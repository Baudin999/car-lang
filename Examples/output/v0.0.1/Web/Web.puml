class "Response a" as Response << (T,orchid) >> {
	Body: a 
}
class "Customer" as Customer  {
	Name: String 
	Number: String 
}
class "CustomerResponse " as CustomerResponse  {
	Body: Customer 
}
Customer --> CustomerResponse : Body
Response *-- CustomerResponse: ALIAS
class "Error" as Error  {
	Code: String 
	Message: String 
}
class "NotFound " as NotFound <Error> {
	Code: String  <i>from Error</i>
	Message: String  <i>from Error</i>
}
Error <|-- NotFound
class "OtherError " as OtherError <Error> {
	Code: String  <i>from Error</i>
	Message: String  <i>from Error</i>
}
Error <|-- OtherError
abstract "ApiResponse" as ApiResponse {
	CustomerResponse 
	NotFound 
	OtherError 
}
CustomerResponse --* ApiResponse : CustomerResponse
NotFound --* ApiResponse : NotFound
OtherError --* ApiResponse : OtherError