{
  "name": "car-lang",
  "version": "1.1.0",
  "description": "The language to rule dragons",
  "main": "dist/ckc.js",
  "repository": "https://github.com/Baudin999/car-lang.git",
  "author": "Carlos Kelkboom <ckelkboom@protonmail.com>",
  "license": "MIT",
  "private": false,
  "bin": "./dist/ckc.js",
  "scripts": {
    "test": "jest --watch",
    "run": "ts-node ./src/ckc.ts -p ./Examples",
    "watch": "ts-node ./src/ckc.ts -p ./Examples -w",
    "build": "tsc --project ./; mkdir ./dist/deflate/; cp ./src/deflate/deflate.js ./dist/deflate/",
    "package": "yarn pkg . --out-path ./lib/",
    "railroad": "ts-node ./src/railroad.ts",
    "move": "cp ./lib/car-lang-macos /usr/local/bin/ckc",
    "clean": "rm -rf ./dist; rm -rf ./lib",
    "go": "yarn clean; yarn build; yarn railroad; yarn package; yarn move",
    "open": "ts-node ./src/ckc.ts -p ./Examples -o",
    "ci:build": "yarn clean; yarn build; yarn railroad; yarn package"
  },
  "dependencies": {
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.122",
    "@types/ramda": "^0.26.1",
    "app-root-path": "^2.1.0",
    "chevrotain": "^4.2.0",
    "chokidar": "^2.1.2",
    "commander": "^2.19.0",
    "fs-extra": "^7.0.1",
    "jest": "^24.3.1",
    "lodash": "^4.17.11",
    "node-fetch": "^2.3.0",
    "open": "^6.0.0",
    "pkg": "^4.3.7",
    "pretty-data": "^0.40.0",
    "ramda": "^0.26.1",
    "string-hash": "^1.1.3",
    "ts-jest": "^24.0.0",
    "tslib": "^1.9.3",
    "typescript": "^3.3.3333"
  },
  "devDependencies": {
    "ts-node": "^8.0.3"
  },
  "pkg": {
    "scripts": [
      "./dist/**/*.js",
      "./deflate.js",
      "./node_modules/chevrotain/lib/chevrotain.min.js"
    ],
    "assets": [
      "./src/html/hljs.js",
      "./src/html/hljs.xcode.css",
      "./src/html/hljs.default.css"
    ]
  }
}
